<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="kr.ac.uc.chatting.dao.IUserAccountDAO">
    <select id="getUserID" resultType="kr.ac.uc.chatting.dto.UserAccountDTO">
        select * from user_account where id=#{param1};
    </select>

    <insert id="saveUserAccount" parameterType="kr.ac.uc.chatting.dto.UserAccountDTO">
        insert into user_account(id, password, authority, nickname, enabled)
                    values (#{id}, #{password}, #{authority}, #{nickname}, #{enabled});
    </insert>
</mapper>